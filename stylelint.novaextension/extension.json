{
  "identifier": "sciencefidelity.stylelint",
  "name": "Stylelint",
  "organization": "Matt Cook",
  "description": "Stylelint language server integration for Nova",
  "version": "0.1.0",
  "categories": ["issues", "formatters", "languages"],
  "license": "MIT",
  "repository": "https://github.com/sciencefidelity/Nova-Stylelint",
  "bugs": "https://github.com/sciencefidelity/Nova-Stylelint/issues",
  "homepage": "https://mattcook.dev",
  "keywords": ["stylelint", "css", "scss", "sass", "less"],
  "main": "main.dist.js",
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:sass",
    "onLanguage:less",
    "onLanguage:html",
    "onWorkspaceContains:package.json",
    "onWorkspaceContains:.stylelintrc",
    "onWorkspaceContains:.stylelintrc.json",
    "onWorkspaceContains:.stylelint.config.js",
    "onWorkspaceContains:.stylelint.config.cjs",
    "onWorkspaceContains:.stylelintrc.yaml",
    "onWorkspaceContains:.stylelintrc.yml"
  ],
  "entitlements": {
    "process": true,
    "filesystem": "readwrite"
  },


  "commands": {
    "extensions": [
      {
        "title": "Preferences",
        "command": "sciencefidelity.astro.openWorkspaceConfig"
      },
      {
        "title": "Restart Server",
        "command": "sciencefidelity.astro.reload"
      },
      {
        "title": "Force Unlock Dependency Installation",
        "command": "sciencefidelity.astro.forceClearLock"
      }
    ]
  }
}
