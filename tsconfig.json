{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["jest", "node", "nova-editor-node"]
  },
  "include": ["./src"]
}
